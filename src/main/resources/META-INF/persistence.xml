    <class>Entities.TutorEntity</class>
    <class>Entities.Intern</class>
    <class>Entities.Excel</class>
    <class>Entities.Internshipinfo</class>
    <class>Entities.Company</class>
    <class>Entities.Schoolgroup</class>
    <class>Entities.AssignEntity</class>
    <class>Entities.KeywordEntity</class>
    <class>Entities.Keyword</class>
    <class>Entities.Tutor</class>
    <class>Entities.Assign</class>
    <class>Entities.InternEntity</class>
    <class>Entities.ExcelEntity</class>
    <class>Entities.SchoolgroupEntity</class>
    <class>Entities.InternshipinfoEntity</class>
    <class>Entities.CompanyEntity</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/ST2EEDB"/>
      <property name="javax.persistence.jdbc.user" value="adm"/>
      <property name="javax.persistence.jdbc.password" value="adm"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="eclipselink.logging.level" value="SEVERE"/>
      <property name="eclipselink.ddl-generation" value="create-or-extend-tables"/>
    </properties>
  </persistence-unit>
</persistence>
